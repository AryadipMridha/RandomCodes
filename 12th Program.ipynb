    "#checking whether a number is abundant or notðŸ˜ƒ\n",
    "num=int(input(\"Enter a number\"))\n",
    "divisor=0\n",
    "abundance=0\n",
    "\n",
    "for i in range(1,num):\n",
    "    if num%i==0:\n",
    "        divisor+=i\n",
    "    if divisor>num:\n",
    "        abundance=1\n",
    "if abundance==1:\n",
    "    print(\"Abundant\")\n",
    "else:\n",
    "    print(\"Not abundant\")\n"

    "#Check whether its automorphic or not\n",
    "num=int(input('Enter number:'))\n",
    "sq=num**2\n",
    "#logic\n",
    "n=len(str(num))\n",
    "last=sq%pow(10,n)\n",
    "#driver code\n",
    "if num==last:\n",
    "    print(\"The number is automorphic.\")\n",
    "else:\n",
    "    print ('not automorphic')"
   ]

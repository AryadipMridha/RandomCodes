    "#Program to find the simple interest\n",
    "def simple_interest(p,r,t):\n",
    "    print('The principle is:',p)\n",
    "    print('The rate is:',r)\n",
    "    print('The time is:',t)\n",
    "    si=(p*r*t)/100;\n",
    "    print('Simple interest is:',si)\n",
    "    return si\n",
    "#driver code\n",
    "simple_interest(100,2,1)\n",
